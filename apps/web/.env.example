# Environment variables template
# Copy this file to .env and fill in your actual values

# API (optional; defaults to http://localhost:3000/api/v1)
# VITE_API_URL=http://localhost:3000/api/v1

# Firebase Configuration (required for Google Sign-In)
# Get these from Firebase Console: https://console.firebase.google.com/ -> Project Settings -> Your apps
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id-here
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id-here
VITE_FIREBASE_APP_ID=your-app-id-here
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id-here
